generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

enum Role {
  ADMIN
  BUSINESS
  TALENT
}

model User {
  id        String   @id // Clerk ID
  email     String   @unique
  role      Role     @default(TALENT)
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
